{
  "name": "osintdash",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_NO_WARNINGS=1 node server.js",
    "dev:webpack": "next dev",
    "start": "NODE_NO_WARNINGS=1 NODE_ENV=production node server.js",
    "lint": "next lint",
    "setup-db": "curl -X GET http://localhost:3001/api/db/direct-setup",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "build": "NEXT_SKIP_LINTING=true next build",
    "build:safe": "npm run lint && npm run typecheck && next build",
    "prebuild": "echo \"Skipping pre-build checks temporarily\"",
    "build:clean": "rm -rf .next && npm run build",
    "analyze": "ANALYZE=true next build",
    "lint-build": "node scripts/lint-and-build.js",
    "fix-code-quality": "npm run format && npm run lint:fix",
    "build:production": "npm run lint:fix && npm run typecheck && next build",
    "// Deployment Builds": "----------",
    "vercel-build": "NODE_OPTIONS='--max-old-space-size=4096' NEXT_PUBLIC_VERCEL_ENV=production VERCEL_ENV=build next build || (echo 'Build failed! Trying again with debug logs:' && NODE_OPTIONS='--max-old-space-size=4096' NEXT_PUBLIC_VERCEL_ENV=production VERCEL_ENV=build DEBUG=* next build)",
    "// Testing": "----------",
    "test": "jest",
    "// Utilities": "----------",
    "clean": "rm -rf .next node_modules",
    "postinstall": "[ -d .git ] && git config core.hooksPath .github/hooks || true"
  },
